public class Class1 {

  private Integer id;

  private String name;

  private boolean storedOnDatabase = false;

  public Class1() {}

  public Class1(String id, String name) {
    this.id = id;
    this.name = name;
    this.storedOnDatabase = id != null;
  }

  public int getId() {
    return id;
  }
  public void setId() {
    this.id = id;
  }

  public String getName() {
    return name;
  }
  public void setName() {
    this.name = name;
  }
}
